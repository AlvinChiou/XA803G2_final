 CREATE TABLE PROD_ITEM(
 ITEMNO                 NUMBER(20), 
 ITEMQTY                NUMBER(2), 
 ITEMMEMO               VARCHAR2(300), 
 ORDNO                  VARCHAR2(25), 
 PRONO                  NUMBER,
 CONSTRAINT PROD_ITEM_PRIMARY_KEY PRIMARY KEY(ITEMNO),
 CONSTRAINT PROD_ITEM_ORDNO_FK FOREIGN KEY(ORDNO) REFERENCES PRO_ORDER(ORDNO),
 CONSTRAINT PROD_ITEM_PRONO_FK FOREIGN KEY(PRONO) REFERENCES PRODUCT(PRONO));
 
 CREATE SEQUENCE PROD_ITEM_seq
 INCREMENT BY 1
 START WITH 2001
 NOMAXVALUE
 NOCYCLE
 NOCACHE;
 
