建立表格: PROD_ITEM, PRO_ORDER, PRODUCT, LOST, NEWS, POWER, FUNCTION_item, APPOINTMENT, SHIFT, DOCTOR, PET, ACT, EMPLOYEE, MEMBER
---------------------------------------------------------------------------------------

alter session set deferred_segment_creation = false;

DROP TABLE PROD_ITEM;
DROP TABLE PRO_ORDER;
DROP TABLE PRODUCT;
DROP TABLE LOST;
DROP TABLE NEWS;
DROP TABLE POWER;
DROP TABLE FUNCTION_item;
DROP TABLE APPOINTMENT;
DROP TABLE SHIFT;
DROP TABLE DRRECORD;
DROP TABLE DOCTOR;
DROP TABLE PET;
DROP TABLE ACTREGISTER;
DROP TABLE ACT;
DROP TABLE GB;
DROP TABLE EMPLOYEE;
DROP TABLE MEMBER;

DROP SEQUENCE PROD_ITEM_seq;
DROP SEQUENCE PRO_ORDER_seq;
DROP SEQUENCE PRODUCT_seq;
DROP SEQUENCE lost_seq;
DROP SEQUENCE news_seq;
DROP SEQUENCE func_seq;
