 create table ACTREGISTER( 
 actRegNo                 VARCHAR2(10) not null,
 actRegName               VARCHAR2(20) not null,
 actRegDate               TIMESTAMP not null,
 actRegTime               TIMESTAMP not null,
 actRegPayState           VARCHAR2(3),
 memNo                    VARCHAR2(10),
 actNo                    VARCHAR2(10),
 CONSTRAINT ACTREGISTER_PRIMARY_KEY PRIMARY KEY(actRegNo),
 CONSTRAINT ACTREGISTER_memNo_FK FOREIGN KEY (memNO) REFERENCES MEMBER (memNo),
 CONSTRAINT ACTREGISTER_actNo_FK FOREIGN KEY (actNo) REFERENCES ACT (actNo));


CREATE SEQUENCE ACTREGISTER_seq
INCREMENT BY 1
START WITH 1
NOMAXVALUE
