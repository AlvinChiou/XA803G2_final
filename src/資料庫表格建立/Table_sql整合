 CONSTRAINT DRRECORD_drNo_FK FOREIGN KEY (drNo) REFERENCES DOCTOR (drNo) ,
 CONSTRAINT DRRECORD_petNo_FK FOREIGN KEY (petNo) REFERENCES PET (petNo));

