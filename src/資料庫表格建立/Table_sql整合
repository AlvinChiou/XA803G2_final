 actDeposit       NUMBER(5) not null,
 actHostFee       NUMBER(5) not null,
 actRegFee        NUMBER not null,
 memNo            VARCHAR2(10),
 empNo            VARCHAR2(10),
 CONSTRAINT ACT_PRIMARY_KEY PRIMARY KEY(actNo),
 CONSTRAINT ACT_memNo_FK FOREIGN KEY(memNO) REFERENCES MEMBER (memNo),
 CONSTRAINT ACT_empNo_FK FOREIGN KEY(empNo) REFERENCES EMPLOYEE (empNo));

