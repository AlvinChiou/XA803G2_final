 CREATE TABLE GB(
 gbNo            VARCHAR2(10) NOT NULL,
 gbTitle         VARCHAR2(200) NOT NULL,
 gbContent       VARCHAR2(1000) NOT NULL,
 gbTime          DATE,
 lostno          VARCHAR2(10) NOT NULL,
 memNo           VARCHAR2(10) NOT NULL,
 CONSTRAINT GB_PRIMARY_KEY PRIMARY KEY(gbNo),
 CONSTRAINT GB_lost_No_FK FOREIGN KEY (lostNo) REFERENCES LOST (lostNo),
 CONSTRAINT GB_mem_No_FK FOREIGN KEY (memNo) REFERENCES MEMBER (memNo));
 
 CREATE SEQUENCE lost_seq
 INCREMENT BY 1
 START WITH 3001
 NOMAXVALUE
 NOCYCLE
 NOCACHE;
 
