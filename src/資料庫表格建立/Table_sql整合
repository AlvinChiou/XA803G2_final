 CREATE TABLE PRO_ORDER(
 ORDNO                  VARCHAR2(25) NOT NULL,
 ORDTIME                TIMESTAMP DEFAULT SYSTIMESTAMP,
 ORDADDR                VARCHAR2(50) NOT NULL,
 ORDTEL                 VARCHAR2(20) NOT NULL,
 ORDGOTIME              TIMESTAMP DEFAULT SYSTIMESTAMP,
 ORDARRTIME             TIMESTAMP DEFAULT SYSTIMESTAMP,
 ORDDELTIME             TIMESTAMP DEFAULT SYSTIMESTAMP,
 ORDSTATE               INTEGER DEFAULT 0,
 MEMNO                  VARCHAR2(10),
 EMPNO                  VARCHAR2(10),
 CONSTRAINT PRO_ORDER_PRIMARY_KEY PRIMARY KEY(ORDNO),
 CONSTRAINT PRO_ORDER_MEMNO_FK FOREIGN KEY(MEMNO) REFERENCES MEMBER(MEMNO),
 CONSTRAINT PRO_ORDER_EMPNO_FK FOREIGN KEY(EMPNO) REFERENCES EMPLOYEE(EMPNO));
 
 CREATE SEQUENCE PRO_ORDER_seq
 INCREMENT BY 1
 START WITH 1001
 NOMAXVALUE
 NOCYCLE
 NOCACHE;
 
