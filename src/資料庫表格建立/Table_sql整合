 petAge           NUMBER(3,1),
 memNo VARCHAR2(10) ,
 CONSTRAINT PET_PRIMARY_KEY PRIMARY KEY(petNo),
 CONSTRAINT PET_memNo_FK FOREIGN KEY (memNO) REFERENCES MEMBER (memNo));

 CREATE SEQUENCE pet_seq
 INCREMENT BY 1
 START WITH 1
 MINVALUE 0
 NOMAXVALUE
 NOCYCLE
 NOCACHE;

