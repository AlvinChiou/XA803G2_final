 CREATE TABLE PRODUCT(	
 PRONO               NUMBER(10), 
 PRODUCTNAME         VARCHAR2(300), 
 "CATEGORY"          VARCHAR2(100), 
 PRICE               NUMBER(10), 
 IMAGE1              BLOB, 
 IMAGE2              BLOB,
 IMAGE3              NCLOB,	
 QUANTITY            NUMBER(10), 
 MINIMUMQUANTITY     NUMBER(2), 
 STATUS              NUMBER(2) DEFAULT 0, 
 KEYWORD             VARCHAR2(100), 
 DESCRIPTION         VARCHAR2(1000), 
 RELATEDPRODUCTS     VARCHAR2(100), 
 PRIORITY            NUMBER(2), 
 DISCOUNT            FLOAT(126) DEFAULT 1.0, 
 SCORE               NUMBER(10) DEFAULT 0,
 CONSTRAINT PRODUCT_PRIMARY_KEY PRIMARY KEY(PRONO));
 
 CREATE SEQUENCE PRODUCT_seq 
 INCREMENT BY 1 
 START WITH 1 
 NOMAXVALUE
 NOCYCLE
 NOCACHE;

