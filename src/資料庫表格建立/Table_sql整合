 CREATE TABLE EMPLOYEE (
 empNo                 VARCHAR2(10) NOT NULL,
 empName               VARCHAR2(10),
 empBirth              DATE,
 empTel                VARCHAR2(15),
 empSex                VARCHAR2(3),
 empPos                VARCHAR2(20),
 empSalary             NUMBER,
 empArrdate            DATE,
 empOff                DATE,
 empId                 VARCHAR2(10),
 empAdd                VARCHAR2(50),
 empPic                BLOB,
 EMPPASSWORD           VARCHAR2(15),
 CONSTRAINT EMPLOYEE_PRIMARY_KEY PRIMARY KEY(empNo));
 
 CREATE SEQUENCE emp_seq
 INCREMENT BY 1
 START WITH 1001
 NOMAXVALUE
 NOCYCLE
 NOCACHE; 
  
